{
   "name": "nest-typescript-starter",
   "version": "1.0.0",
   "description": "Nest TypeScript starter repository",
   "license": "MIT",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev:db": "bash ./src/scripts/start-db.sh",
      "start:dev": "nodemon --config nodemon.json",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "echo 'No e2e tests implemented yet.'",
      "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-type-orm-config.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "typeorm:migration:generate": "npm run typeorm -- migration:generate -n user",
      "typeorm:migration:run": "npm run typeorm -- migration:run"
   },
   "dependencies": {
      "@nestjs/common": "7.6.14",
      "@nestjs/core": "^7.6.14",
      "@nestjs/passport": "^7.1.5",
      "@nestjs/platform-express": "7.6.14",
      "@nestjs/swagger": "^4.8.0",
      "@nestjs/typeorm": "^7.1.5",
      "class-transformer": "0.4.0",
      "class-validator": "0.13.1",
      "dotenv": "^8.2.0",
      "mysql": "^2.14.1",
      "mysql2": "^2.2.5",
      "passport": "^0.4.1",
      "passport-local": "^1.0.0",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.10",
      "rimraf": "3.0.2",
      "rxjs": "6.6.6",
      "sqlite3": "^5.0.2",
      "swagger-ui-express": "4.1.6",
      "typeorm": "0.2.31"
   },
   "devDependencies": {
      "@nestjs/cli": "7.5.6",
      "@nestjs/schematics": "7.3.0",
      "@nestjs/testing": "7.6.14",
      "@types/express": "4.17.11",
      "@types/node": "^8.0.29",
      "@types/passport-local": "^1.0.33",
      "@types/supertest": "2.0.10",
      "@typescript-eslint/eslint-plugin": "4.19.0",
      "@typescript-eslint/parser": "4.19.0",
      "eslint": "7.22.0",
      "eslint-config-prettier": "8.1.0",
      "eslint-plugin-import": "2.22.1",
      "jest": "26.6.3",
      "nodemon": "^2.0.7",
      "prettier": "2.2.1",
      "supertest": "6.1.3",
      "ts-jest": "26.5.2",
      "ts-loader": "8.0.18",
      "ts-node": "3.3.0",
      "tsconfig-paths": "3.9.0",
      "typescript": "3.3.3333"
   }
}
